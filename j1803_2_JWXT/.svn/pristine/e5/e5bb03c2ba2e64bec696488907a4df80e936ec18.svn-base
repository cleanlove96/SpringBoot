body{
    width:100%;
    height: auto;
    background-color: #EEEEEE;
}

.navTime{
    float: left;
    margin-top: 15px;
}

.navTime span{
    float: left;
}

.navTime div{
    float: right;
    color: blue;
}

.wrap{
    width: 80%;
    height:100%;
    border-left: 1px solid lightgray;
    border-right: 1px solid lightgray;
    margin-left: auto;
    margin-right: auto;
    background-color: #ffffff;
    margin-top: -20px;
}

.wrap nav{
    margin-bottom: 0px;
}

.wrap .container {
    width: 100%;
}

.wrap .container img{
    float: left;
    margin-top: 28px;
    margin-left: 40px;
}

.btn_search{
    width: 230px;
    height: 36px;
    float: right;
    margin-top: 28px;
    margin-right: 40px;
    border: 1px solid #E0E0E0;
    box-shadow: 0 0 4px RGBA(0,0,0,0.2);
}

.btn_search input{
    width: 172px;
    font-size: 13px;
    float: left;
    margin: 8px 0 8px 13px;
    border: none;
}

.btn_search a{
    padding: 7px 14px;
    float: right;
    display: inline-block;
}

.btn_search .searchVerticalLine{
    width: 1px;
    height: 20px;
    margin-top: 7px;
    display: inline-block;
    background-color: #E0E0E0;
}

.footer{
    width: 100%;
    height: auto;
    background-color: gray;
    text-align: center;
    margin: 0 auto;
    padding-top: 30px;
}

.footer .container {
    height: auto;
    margin: 0 auto;
    text-align: center;
    position: relative;
}

.ad li{
    float: left;
}

.logo{
    width: 200px;
    height: 155px;
    text-align: left;
}

.logo img{
    margin-top: 40px;
    margin-bottom: 4px;
}

.logo span{
    font-size: 14px;
    color: #fff;
    display: block;
}

.ad {
    list-style: none;
}

.ad-info {
    width: 555px;
    height: 155px;
    border-left: 1px #ffffff solid;
    border-right: 1px #ffffff solid;
    text-align: left;
    padding-left: 45px;
    margin: 0;
    float: left;
}

.sub-brands{
    float: left;
    width: 105px;
    margin: 0 30px;
    text-align: left;
}

.sub-brands span{
    font-size: 16px;
    color: #fff;
    font-weight: bold;
    display: block;
    margin-bottom: 8px;
}

.sub-brands a{
    font-size: 14px;
    color: #EEEEEE;
    margin-bottom: 6px;
    display: inline-block;
}

.attention {
    width: 246px;
    margin-left: 62px;   
}

.wei{
    margin-left: 20px;
}

.attention b{
    font-size: 14px;
    font-weight: normal;
    color: #EEEEEE;
    margin-left: 14px;
}

.are {
    margin-left: 44px !important;
}

.honour {
    font-size: 24px;
    color: #333;
    text-align: left;
    margin-bottom: 18px;
    margin-top: 38px;
}

.honour span{
    width: 5px;
    height: 27px;
    background: #3f86d6;
    display: inline-block;
    position: relative;
    top: 6px;
    margin-right: 10px;
}

.price {
    width: 100%;
    height: auto;
    display: table;
    padding-left: 0px;
    margin-left:0px;
}

.price li{
	height:220px;
    list-style: none;
    float: left;
    text-align: center;
}

.price img{
	margin:0 auto;
}

.price p{
    font-size: 12px;
    color: #333;
    padding-top: 16px;
}

.places{
	width:100%;
    height: auto;
    margin: 0 auto;
    text-align: center;
    position: relative;
}

.places-title{
    width: 100%;
    height: auto;
    padding-top: 50px;
    padding-bottom: 10px;
}

.places-title p{
    font-size: 36px;
    color: #444;
    line-height: 60px;
}

@media (min-width: 992px){
	.places-title p{
		font-size:px;
	}
}

@media (min-width: 768px){
	.places-title p{
		font-size:28px;
	}
}

@media (min-width: 0px){
	.places-title p{
		font-size:20px;
	}
}



.places-location{
    height: auto;
    margin: 0 auto;
    display: table;
    padding-left:0px;
    text-align:center;
}

/* .places-location li{
    list-style: none;
    float: left;
    width: 30%;
    height: 200px;
    position: relative;
    padding: 0px;
    
}  */

.places-location img{
	margin:0 auto;
	padding-bottom:25px;
} 

.center-img{
    font-size: 18px;
    color: #000;
    height: 40px;
    line-height: 40px;
    background: rgba(255,255,255,0.62);
    position: absolute;
    top: 80px;
    z-index: 5;
    width: 100%;
}

.online-class{
    width: 100%;
    height: auto;
    display: table;
    margin-top: 60px;
}

.high_fen {
    width: 5px;
    height: 38px;
    background: #b53591;
    float: left;
}

.online_zi {
    background: #8392db;
    margin-left: 15px;
    margin-right: 15px;
}

.high_title {
    font-size: 24px;
    color: #333;
    display: block;
    margin-top: -4px;
    margin-left: 14px;
    line-height: 28px;
}

.high_xu {
    font-size: 12px;
    color: #b2b2b2;
}

.ascension_link {
    font-size: 12px;
    color: #aaa;
    float: right;
    margin-top: -6px;
}

.online-class-left {
    width: 22%;
    height: 24%;
    background: url("../images/front_IMG/online01.jpg") center no-repeat;
    float: left;
    text-align: center;
    margin-top: 15px;
    margin-left: 40px;
    float: left;
}

.online-class-left img{
    margin-top: 30px;
}

.online-class-left span{
    font-size: 14px;
    color: #fff;
    display: block;
    line-height: 20px;
    margin-top: 20px;
}

.class-hover{
    margin-top: 15px;
    width: 50%;
    height: 50px;
}

.class-hover em{
    width: 100%;
    height: 80%;
    display: inline-block;
    border: 1px solid #ffffff;
    border-radius: 20px;
    background-color: #ffffff;
    cursor: pointer;
    margin-left: 50%;
}

.class-hover img{
    margin-top: 8%;
}

.online-class-right{
    width: 70%;
    height: 20%;
    background: #ffffff;
    float: left;
    margin-top: 15px;
}

.right-center{
    width: 235px;
    height: 100%;
    float: left;
}

.right-first{
    width: 100%;
    height: 58px;
    margin: 26px 0;
    position: relative;
    align: center;
}

.right-first img{
    position: absolute;
    left: 14px;
    top: 6px;
    z-index: 99;
}

.right-title{
    font-size: 16px;
    color: #333333;
    display: block;
    float: left;
    line-height: 26px;
    margin-top: 8px;
    margin-left: 70px;
}

.right-brief{
    font-size: 12px;
    color: #aaa;
    float: left;
    margin-left: 70px;
}

ul{
    list-style: none;
}

.row li{
    border-right: 1px solid #grey;
}

.row a{
    text-decoration: none;
    cursor: pointer;
}

.navbar-toggle{
	border-color:#eeeeee;
}